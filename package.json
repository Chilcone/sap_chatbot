{
  "name": "sap_chatbot",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^5",
    "@sap/cds-odata-v2-adapter-proxy": "^1.8.4",
    "cf-nodejs-logging-support": "^6.12.0",
    "dotenv": "^15.0.0",
    "express": "^4",
    "follow-redirects": "^1.14.7"
  },
  "devDependencies": {
    "sqlite3": "^5.0.2"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "cds run"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "OP_API_SALES_ORDER_SRV_0001": {
        "kind": "odata-v2",
        "model": "srv\\external\\OP_API_SALES_ORDER_SRV_0001",
        "credentials": {
          "destination": "ERP_IDA_SO_SRV"
        }
      },
      "API_PRODUCT_SRV": {
        "kind": "odata-v2",
        "model": "srv\\external\\API_PRODUCT_SRV",
        "credentials": {
          "destination": "ERP_IDA_PRO_SRV"
        }
      },
      "API_BILLING_DOCUMENT_SRV": {
        "kind": "odata-v2",
        "model": "srv/external/API_BILLING_DOCUMENT_SRV"
      }
    }
  }
}